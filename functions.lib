MAKE_STRING:
  PUSH(FP);
  MOV(FP, SP);
  PUSH(R1);
  PUSH(R2);
  MOV(R3,SCMNARGS);
  CMP(R3,IMM(2));
  JUMP_EQ(MAKE_STRING_2_ARGS);
  
  MOV(R1,IMM(70));
  MOV(R0,SCMARG(0));
  MOV(R0,INDD(R0,1));
  MOV(R2,R0);
  JUMP(MAKE_STRING_LOOP);
 MAKE_STRING_2_ARGS:
  MOV(R1,SCMARG(0));
  MOV(R1,INDD(R1,1));
  MOV(R0,SCMARG(1));
  MOV(R0,INDD(R0,1));
  MOV(R2,R0);
MAKE_STRING_LOOP:
  CMP(R0,IMM(0));
  JUMP_EQ(MAKE_STRING_EXIT);
  PUSH(R1);
  DECR(R0);
  JUMP(MAKE_STRING_LOOP);
MAKE_STRING_EXIT:
